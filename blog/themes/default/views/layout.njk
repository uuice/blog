<!DOCTYPE html>
{% set hasBanner = "true" %}
{% set previewMode = "false" %}
{% set sidebar_position = "left" %}
<html theme="dark" showBanner="true" hasBanner="{{ hasBanner }}">
<meta charset="UTF-8">
<title>{% SysConfigItem path="title" %}</title>
<link href="https://cdn.staticfile.org/font-awesome/6.4.2/css/fontawesome.min.css" rel="stylesheet">
<link href="https://cdn.staticfile.org/font-awesome/6.4.2/css/brands.min.css" rel="stylesheet">
<link href="https://cdn.staticfile.org/font-awesome/6.4.2/css/solid.min.css" rel="stylesheet">
<script src="/assets/js/color.global.min.js"></script>
<script src="/assets/js/load-settings.js"></script>


{% include '_partial/head.njk' %}
<body>
  {% if previewMode %}
    {# {% include '_widget/color.njk' %} #}
  {% endif %}
  {% if hasBanner != "false" %}
    {% include '_partial/banner.njk' %}
  {% endif %}
  <div id="main-grid" class="{% if sidebar_position == 'right' %} right-sidebar {% endif %}">
    <div id="nav" class="{% if pageType == 'Index' %} is_home {% endif %}">
      {% include '_partial/navbar.njk' %}
    </div>
    <div id="sidebar-wrapper">
      {% include '_partial/sidebar.njk' %}
    </div>
    <div id="content-body">
        {% block body %}{% endblock %}
    </div>
    <div id="footer-wrapper">
      {% include '_partial/footer.njk' %}
    </div>
    {% include '_partial/back-to-top.njk' %}
  </div>
  <script src="/assets/js/light-dark-switch.js"></script>
</body>
</html>